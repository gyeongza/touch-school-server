generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model School {
  id        Int      @id @default(autoincrement())
  name      String   // SCHUL_NM (학교명)
  address   String?  // ORG_RDNMA (도로명주소)
  createdAt DateTime @default(now())
}